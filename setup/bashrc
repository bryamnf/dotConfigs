#/bin/bash

#######Bashrc set up###########
path="$HOME/dotconfigs/setup"
if [ -f "$path/shell_config" ] && [ -f "$path/ishell_config" ]; then
    source "$path/shell_config"
    if echo $- | grep -q "i"; then
        source "$path/ishell_config"
        echo "Using dotconfigs in interactive shell mode!"
        cat "$path/ascii_art"
    else
        echo "Using dotconfigs in non-interactive shell mode!"
    fi
fi
##############################

