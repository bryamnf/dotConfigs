#/bin/bash -l

#######Bashrc set up###########
if [ -f "$HOME/dotconfigs/setup/shell_config.sh" ]; then
  case $- in
    *i*)
       echo "Interactive shell"
       source "$HOME/dotconfigs/setup/ishell_config.sh" 
      ;;
    *) ;;
  esac
  source "$HOME/dotconfigs/setup/shell_config.sh" 
  echo "Using dotconfigs"
fi
##############################

######MODULES#######
source /usr/share/modules/init/bash
####################
